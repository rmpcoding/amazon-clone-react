et\n        dispatch({\n            type: \"REMOVE_FROM_BASKET\",\n            id\n        })\n    }\n    return (\n        <div className=\"checkoutProduct\">\n            <img className=\"checkoutProduct__image\" src={image} alt=\"Product\" />\n            <div className=\"checkoutProduct__info\">\n                <p className=\"checkoutProduct__title\">{title}</p>\n                <p className=\"checkoutProduct__price\">\n                    <small>$</small>\n                    <strong>{price}</strong>\n                </p>\n\n                <div className=\"CheckoutProduct__rating\">\n                    {Array(rating)\n                        .fill()\n                        .map((_, i) => (\n                            <span>⭐️</span>\n                        ))}\n                </div>\n                <button onClick={removeFromBasket}>Remove from Basket</button>\n            </div>\n        </div>\n    );\n};\n\nexport default CheckoutProduct;\n","/Users/robertsmac/Documents/Developer/JavaScript-Projects/react-clones/amazon-clone/src/Login/Login.js",[],"/Users/robertsmac/Documents/Developer/JavaScript-Projects/react-clones/amazon-clone/src/firebase.js",[],{"ruleId":"71","replacedBy":"72"},{"ruleId":"73","replacedBy":"74"},{"ruleId":"75","severity":1,"message":"76","line":12,"column":12,"nodeType":"77","messageId":"78","endLine":12,"endColumn":16},{"ruleId":"79","severity":1,"message":"80","line":33,"column":8,"nodeType":"81","endLine":33,"endColumn":10,"suggestions":"82"},{"ruleId":"83","severity":1,"message":"84","line":10,"column":30,"nodeType":"85","messageId":"86","endLine":10,"endColumn":38},{"ruleId":"71","replacedBy":"87"},{"ruleId":"73","replacedBy":"88"},{"ruleId":"83","severity":1,"message":"89","line":6,"column":14,"nodeType":"85","messageId":"86","endLine":6,"endColumn":20},{"ruleId":"83","severity":1,"message":"84","line":8,"column":30,"nodeType":"85","messageId":"86","endLine":8,"endColumn":38},{"ruleId":"83","severity":1,"message":"84","lin